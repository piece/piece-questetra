{
    "name": "piece/piece-questetra",
    "type": "symfony-bundle",
    "description": "A client for Questetra BPM Suite API",
    "keywords": ["questetra", "bpm", "workflow"],
    "homepage": "http://piece-framework.com/projects/piece-questetra",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "KUBO Atsuhiro",
            "email": "kubo@iteman.jp"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "guzzle/guzzle": ">=2.0.0"
    },
    "require-dev": {
        "symfony/config": ">=2.0.0",
        "symfony/dependency-injection": ">=2.0.0",
        "symfony/http-kernel": ">=2.0.0",
        "symfony/yaml": ">=2.0.0"
    },
    "suggest": {
        "symfony/config": ">=2.0.0",
        "symfony/dependency-injection": ">=2.0.0",
        "symfony/http-kernel": ">=2.0.0",
        "symfony/yaml": ">=2.0.0"
    },
    "autoload": {
        "psr-0": {
            "Piece\\Questetra": ""
        }
    },
    "target-dir" : "Piece/Questetra",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
